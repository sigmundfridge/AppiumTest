{
  "name": "AppiumTest",
  "version": "0.0.1",
  "scripts": {
    "start": "react-native start",
    "windows": "react-native-winjs start",
    "windows:build": "node ./scripts/winjsBundle.js",
    "windows:buildandcopy": "npm run windows:build && cp -rf ./windows/AxsyApp ~/Documents/",
    "windows:make-release": "node ./scripts/makeWindowsRelease.js",
    "npm-reinstall": "bash ./npm-reinstall.sh",
    "test": "rm -rf ./node_modules/jest-cli/.haste_cache && jest --verbose",
    "test:watch": "npm run test -- --watch",
    "test-chrome": "node-debug --preload false --nodejs --harmony jest --runInBand",
    "postinstall": "rn-nodeify --exclude --browser --core crypto,stream,vm",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "clear-cache": "watchman watch-del-all && ./node_modules/react-native/packager/packager.sh start --reset-cache",
    "make-icon-font": "node ./scripts/makeIconFont.js",
    "flow": "flow",
    "eslint": "eslint app/**",
    "eslint-fix": "eslint --fix app/**"
  },
  "jest": {
    "scriptPreprocessor": "jestSupport/scriptPreprocess.js",
    "setupEnvScriptFile": "node_modules/react-native/jestSupport/env.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "packager/react-packager/src/Activity/"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils",
      "promise",
      "source-map",
      "key-mirror",
      "immutable",
      "fetch",
      "redux",
      "redux-thunk",
      "fbjs"
    ]
  },
  "dependencies": {
    "babel-cli": "^6.8.0",
    "buffer": "git+https://github.com/axsy-dev/buffer#master",
    "lodash": "^3.10.1",
    "node-uuid": "^1.4.7",
    "react": "0.14.8",
    "react-native": "0.23.0",
    "react-native-crypto": "^2.0.0",
    "react-native-fs": "1.1.0",
    "react-native-sqlcipher-storage": "git+https://github.com/axsy-dev/react-native-sqlcipher-storage#master",
    "react-native-vector-icons": "1.1.0",
    "react-native-winjs": "0.1.6",
    "react-redux": "4.1.1",
    "react-relay": "^0.8.1",
    "react-timer-mixin": "^0.13.3",
    "readable-stream": "^1.0.33",
    "redux": "^3.1.2",
    "redux-thunk": "^1.0.3",
    "reselect": "^2.5.1",
    "uuid": "^2.0.2",
    "webpack-utf8-bom": "^1.0.3",
    "immutable": "3.8.1"
  },
  "devDependencies": {
    "art": "git+https://github.com/chukcha-wtf/art#master",
    "babel-core": "6.8.0",
    "babel-eslint": "6.1.0",
    "babel-jest": "6.0.1",
    "babel-loader": "6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-relay-plugin": "0.8.1",
    "css-loader": "0.23.1",
    "d3": "3.5.12",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-flow-vars": "^0.4.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.8.5",
    "flow-bin": "0.26.0",
    "glob": "^7.0.3",
    "haste-resolver-webpack-plugin": "^0.1.2",
    "iconv-lite": "^0.4.13",
    "imports-loader": "^0.6.5",
    "istanbul": "^0.4.2",
    "jest-cli": "^0.8.2",
    "json-loader": "^0.5.2",
    "moment": "^2.10.6",
    "react-addons-test-utils": "^0.14.7",
    "react-art": "git+https://github.com/chukcha-wtf/react-art#render-fix",
    "react-dom": "~0.14.7",
    "react-hot-loader": "^1.2.7",
    "rn-nodeify": "git+https://github.com/axsy-dev/rn-nodeify#master",
    "shelljs": "^0.7.0",
    "url-loader": "^0.5.7",
    "vs-fix-sourcemaps": "^0.1.7",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0",
    "webpack-html-plugin": "^0.1.1",
    "zip-dir": "^1.0.2"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify",
    "vm": "vm-browserify"
  }
}
